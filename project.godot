config_version=5

[application]

config/name="Project Demon Sword"
config/version="0.0.1"
run/main_scene="res://levels/world.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="project_demon_sword"
config/features=PackedStringArray("4.3", "Forward Plus")
run/max_fps=60
config/icon="res://icon.svg"

[autoload]

Utils="*res://singletons/utils.gd"
Stats="*res://singletons/stats.gd"
SaveAndLoad="*res://save_and_load/save_and_load.gd"
